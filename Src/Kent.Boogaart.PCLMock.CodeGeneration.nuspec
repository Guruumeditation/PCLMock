<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>Kent.Boogaart.PCLMock.CodeGeneration</id>
    <version>@build.number@</version>
    <authors>Kent Boogaart</authors>
    <owners>Kent Boogaart</owners>
    <licenseUrl>https://github.com/kentcb/PCLMock/blob/master/LICENSE</licenseUrl>
    <projectUrl>https://github.com/kentcb/PCLMock</projectUrl>
